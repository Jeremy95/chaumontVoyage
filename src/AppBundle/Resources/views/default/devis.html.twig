{% extends 'base.html.twig' %}

{% block body %}

    <section id="content" style="margin-bottom: 0px;">

        <div class="content-wrap">

            <div class="container clearfix">

                <!-- Contact Form
                ============================================= -->
                <div class="fancy-title title-dotted-border text-center">
                    <h3>Devis Transport</h3>
                </div>

                {{ form_start(form) }}
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.title) }}
                        {{ form_widget(form.title) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.email) }}
                        {{ form_widget(form.email) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.nickname) }}
                        {{ form_widget(form.nickname) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.phone) }}
                        {{ form_widget(form.phone) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.name) }}
                        {{ form_widget(form.name) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.address) }}
                        {{ form_widget(form.address) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.city) }}
                        {{ form_widget(form.city) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.society) }}
                        {{ form_widget(form.society) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.departDate) }}
                        {{ form_widget(form.departDate) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.departHour) }}
                        {{ form_widget(form.departHour) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.arrivedDate) }}
                        {{ form_widget(form.arrivedDate) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.arrivedHour) }}
                        {{ form_widget(form.arrivedHour) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.comments) }}
                        {{ form_widget(form.comments) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.destination) }}
                        {{ form_widget(form.destination) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.equipment) }}
                        {{ form_widget(form.equipment) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.capacity) }}
                        {{ form_widget(form.capacity) }}
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col">
                        {{ form_label(form.locationTake) }}
                        {{ form_widget(form.locationTake) }}
                    </div>
                    <div class="col">
                        {{ form_label(form.files) }}
                        {{ form_widget(form.files) }}
                    </div>
                </div>
                <div class="form-group row">
                    {{ form_widget(form.submit) }}
                </div>
                {{ form_widget(form) }}
                {{ form_rest(form) }}
                {{ form_end(form) }}
                {{ form(form) }}
            </div>
        </div>

    </section>

{% endblock %}
