                            </a>
                        </div>
                        <div class="fbox-desc">
                            <h3>{{ pageAutocarsTti.content.titleTwo }}<span class="subtitle"></span></h3>
                            <p>{{ pageAutocarsTti.content.contentTwo }}</p>
                        </div>
                    </div>
                </div>

                <div class="col_one_third nobottommargin col_last">
                    <div class="feature-box media-box">
                        <div class="fbox-media">
                            <a href="#securite">
                                <img
                                        {% if pageAutocarsTti.imageContent.imageThree.basename is not defined %}
                                            src="{{ asset('images/upload/autocars-tti/' ~ pageAutocarsTti.imageContent.imageThree.basename) }}"
                                        {% else %}
                                            src="{{ asset('images/ceinture-securite.jpg') }}"
                                        {% endif %}
